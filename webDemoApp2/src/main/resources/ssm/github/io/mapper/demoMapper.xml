<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ssm.github.io.mapper.demoMapper">
	<resultMap type="TbBrand" id="demoMapper">
	</resultMap>

	<insert id="addOne">
		insert into study002jdbc.tb_brand
		values (#{brandName}, #{companyName}, #{ordered}, #{description}, #{status});
	</insert>

	<select id="selectById" resultType="ssm.github.io.pojo.TbBrand">
	</select>
</mapper>
